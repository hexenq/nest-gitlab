{
  "name": "nest-gitlab",
  "version": "1.0.2",
  "description": "A node-gitlab wrapper for NestJS framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --fix --project tsconfig.json",
    "clean": "rimraf dist",
